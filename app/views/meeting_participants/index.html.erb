<h1>Listing meeting_participants</h1>

<table>
  <tr>
    <th>Meeting</th>
    <th>User</th>
    <th>Created by</th>
    <th>Updated by</th>
  </tr>

<% for meeting_participant in @meeting_participants %>
  <tr>
    <td><%=h meeting_participant.meeting_id %></td>
    <td><%=h meeting_participant.user_id %></td>
    <td><%=h meeting_participant.created_by %></td>
    <td><%=h meeting_participant.updated_by %></td>
    <td><%= link_to 'Show', meeting_participant %></td>
    <td><%= link_to 'Edit', edit_meeting_participant_path(meeting_participant) %></td>
    <td><%= link_to 'Destroy', meeting_participant, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New meeting_participant', new_meeting_participant_path %>
