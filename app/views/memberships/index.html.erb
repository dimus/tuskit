<h1>Listing memberships</h1>

<table>
  <tr>
  </tr>
  
<% for membership in @memberships %>
  <tr>
    <td><%= link_to 'Show', membership_path(membership) %></td>
    <td><%= link_to 'Edit', edit_membership_path(membership) %></td>
    <td><%= link_to 'Destroy', membership_path(membership), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New membership', new_membership_path %>